<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
xmlns:context = "http://www.springframework.org/schema/context"
xsi:schemaLocation = "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd"> 

	<bean id="helloWorld" class="com.spring.HelloWorld">
		<property name="message1" value="Hello World"></property>
		<property name="message2" value="Bye World"></property>
	</bean>
	
	<bean id="helloIndia" class="com.spring.HelloIndia">
		<property name="message1" value="Hey india"></property>
		<property name="message2" value="Bye india"></property>
	</bean> 
</beans>